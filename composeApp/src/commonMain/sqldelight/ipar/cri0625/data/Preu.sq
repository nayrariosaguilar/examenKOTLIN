CREATE TABLE IF NOT EXISTS Preu (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    preu TEXT NOT NULL,
    sessio INTEGER,
    zona INTEGER,
     FOREIGN KEY(sessio) REFERENCES Session(dia),
     FOREIGN KEY(zona) REFERENCES Zona(codi)
);
insert:
INSERT INTO Preu(preu,sessio, zona) VALUES (?,?,?);

select:
SELECT * FROM Preu;